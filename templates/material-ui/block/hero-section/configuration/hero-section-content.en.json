{
  "$schema": "https://schema.croct.com/json/v1/template-content.json",
  "type": "structure",
  "attributes": {
    "form": {
      "type": "structure",
      "attributes": {
        "ctaLabel": {
          "type": "text",
          "value": {
            "type": "static",
            "value": "Start now"
          }
        },
        "inputLabel": {
          "type": "text",
          "value": {
            "type": "static",
            "value": "Your email address"
          }
        }
      }
    },
    "note": {
      "type": "text",
      "value": {
        "type": "static",
        "value": "By clicking \"Start now\" you agree to our [Terms & Conditions](#)."
      }
    },
    "images": {
      "type": "structure",
      "attributes": {
        "dark": {
          "type": "text",
          "value": {
            "type": "static",
            "value": "https://mui.com/static/screenshots/material-ui/getting-started/templates/dashboard-dark.jpg"
          }
        },
        "light": {
          "type": "text",
          "value": {
            "type": "static",
            "value": "https://mui.com/static/screenshots/material-ui/getting-started/templates/dashboard.jpg"
          }
        }
      }
    },
    "heading": {
      "type": "text",
      "value": {
        "type": "static",
        "value": "Our latest **products**"
      }
    },
    "tagline": {
      "type": "text",
      "value": {
        "type": "static",
        "value": "Explore our cutting-edge dashboard, delivering high-quality solutions tailored to your needs. Elevate your experience with top-tier features and services."
      }
    }
  }
}
