{
  "$schema": "https://schema.croct.com/json/v1/template-content-schema.json",
  "type": "structure",
  "attributes": {
    "title": {
      "type": {
        "type": "text",
        "minimumLength": 1
      },
      "label": "Title",
      "private": false,
      "optional": false,
      "position": 0,
      "description": "The section title."
    },
    "highlights": {
      "type": {
        "type": "list",
        "items": {
          "type": "structure",
          "attributes": {
            "icon": {
              "type": {
                "type": "text",
                "choices": {
                  "QueryStatsRounded": {
                    "label": "Query Stats ",
                    "position": 5
                  },
                  "ThumbUpAltRounded": {
                    "label": "Thumb Up ",
                    "position": 2
                  },
                  "AutoFixHighRounded": {
                    "label": "Auto Fix ",
                    "position": 3
                  },
                  "ConstructionRounded": {
                    "label": "Construction ",
                    "position": 1
                  },
                  "SupportAgentRounded": {
                    "label": "Support Agent ",
                    "position": 4
                  },
                  "SettingsSuggestRounded": {
                    "label": "Settings ",
                    "position": 0
                  }
                }
              },
              "label": "Icon",
              "private": false,
              "optional": false,
              "position": 0,
              "description": "The icon related to the highlight."
            },
            "title": {
              "type": {
                "type": "text",
                "minimumLength": 1
              },
              "label": "Title",
              "private": false,
              "optional": false,
              "position": 1,
              "description": "The highlight title."
            },
            "description": {
              "type": {
                "type": "text",
                "minimumLength": 1
              },
              "label": "Description",
              "private": false,
              "optional": false,
              "position": 2,
              "description": "The highlight description."
            }
          }
        },
        "minimumLength": 1,
        "maximumLength": 6,
        "itemLabel": "Highlight"
      },
      "label": "Highlights",
      "private": false,
      "optional": false,
      "position": 2,
      "description": "The list of highlights."
    },
    "description": {
      "type": {
        "type": "text",
        "format": "multiline",
        "minimumLength": 1
      },
      "label": "Description",
      "private": false,
      "optional": true,
      "position": 1,
      "description": "The section description."
    }
  }
}
