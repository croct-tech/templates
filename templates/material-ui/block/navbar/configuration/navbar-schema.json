{
  "$schema": "https://schema.croct.com/json/v1/template-content-schema.json",
  "type": "structure",
  "title": "Navbar",
  "attributes": {
    "items": {
      "type": {
        "type": "list",
        "items": {
          "type": "structure",
          "attributes": {
            "label": {
              "type": {
                "type": "text",
                "minimumLength": 1
              },
              "label": "Label",
              "private": false,
              "optional": false,
              "position": 0,
              "description": "The item label."
            },
            "link": {
              "type": {
                "type": "text",
                "minimumLength": 1
              },
              "label": "Link",
              "private": false,
              "optional": false,
              "position": 1,
              "description": "The item link or anchor."
            }
          }
        },
        "maximumLength": 8,
        "minimumLength": 1
      },
      "label": "Items",
      "private": false,
      "optional": false,
      "position": 0,
      "description": "A list of links to be display on the navbar."
    },
    "ctas": {
      "type": {
        "type": "list",
        "items": {
          "id": "cta",
          "type": "reference"
        },
        "maximumLength": 2,
        "minimumLength": 1
      },
      "label": "CTAs ",
      "private": false,
      "optional": false,
      "position": 1,
      "description": "The CTAs of the navbar."
    }
  },
  "description": "The navigation bar at the top of a page."
}
