{
  "$schema": "https://schema.croct.com/json/v1/template-content-schema.json",
  "type": "structure",
  "attributes": {
    "content": {
      "type": {
        "type": "list",
        "items": {
          "type": "structure",
          "attributes": {
            "icon": {
              "type": {
                "type": "text",
                "choices": {
                  "ThumbUp": {
                    "label": "Thumb up alt icon",
                    "position": 2
                  },
                  "Construction": {
                    "label": "Construction icon",
                    "position": 1
                  },
                  "SettingsSuggested": {
                    "label": "Settings suggest icon",
                    "position": 0
                  },
                  "AutoFixHighRounded": {
                    "label": "Auto fix high icon",
                    "position": 3
                  }
                }
              },
              "label": "Icon",
              "private": false,
              "optional": false,
              "position": 0,
              "description": "The icon related to the item."
            },
            "title": {
              "type": {
                "type": "text",
                "minimumLength": 1
              },
              "label": "Title",
              "private": false,
              "optional": false,
              "position": 1,
              "description": "The title of the item."
            },
            "description": {
              "type": {
                "type": "text",
                "format": "multiline",
                "minimumLength": 1
              },
              "label": "Description",
              "private": false,
              "optional": false,
              "position": 2,
              "description": "The description of the item."
            }
          }
        },
        "maximumLength": 4,
        "minimumLength": 1
      },
      "label": "Content",
      "private": false,
      "optional": false,
      "position": 1
    }
  }
}
