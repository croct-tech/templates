[
  {
    "pattern": "^template:/(.+?)$",
    "destination": "$1"
  },
  {
    "pattern": "^npm:/(.+)$",
    "destination": "npm/$1"
  },
  {
    "pattern": "^croct:/(.+)$",
    "destination": "croct/$1"
  },
  {
    "pattern": "^nextjs:/(.+)$",
    "destination": "nextjs/$1"
  },
  {
    "pattern": "^shadcn:/(.+)$",
    "destination": "shadcn/$1"
  },
  {
    "pattern": "^magicui:/(.+)$",
    "destination": "magicui/$1"
  },
  {
    "pattern": "^https://magicui.design/r/(.+)$",
    "destination": "magicui/ui/$1"
  }
]
