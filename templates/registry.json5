[
  {
    "pattern": "^npm://?(.+)$",
    "destination": "npm/$1"
  },
  {
    "pattern": "^croct://?(.+)$",
    "destination": "croct/$1"
  },
  {
    "pattern": "^nextjs://?(.+)$",
    "destination": "nextjs/$1"
  },
  {
    "pattern": "^shadcn-ui://?(.+)$",
    "destination": "shadcn-ui/$1"
  },
  {
    "pattern": "^magic-ui://?(.+)$",
    "destination": "magic-ui/$1"
  },
  {
    "pattern": "^https://magicui.design/r/(.+)$",
    "destination": "magic-ui/ui/$1"
  }
]
