{
  "$schema": "https://schema.croct.com/json/v1/catalog-template.json",
  "title": "Header section",
  "description": "A header section using Tailwind components and Croct.",
  "metadata": {
    "id": "library/tailwind-css/header-section",
    "author": {
      "name": "Tailwind",
      "avatarUrl": "https://github.com/tailwindui.png",
      "websiteUrl": "https://tailwindcss.com/?utm_source=croct"
    },
    "documentationUrl": "https://github.com/croct-tech/templates/blob/master/templates/tailwind/block/header-section/README.md",
    "sourceUrl": "https://github.com/croct-tech/templates/blob/master/templates/tailwind/block/header-section/template.json5",
    "coverImageUrl": "https://github.com/croct-tech/templates/blob/master/templates/tailwind/block/header-section/cover.png",
    "categories": [
      "interface/marketing",
      "interface/section",
      "library/tailwind-css",
      "use-case/saas"
    ],
    "relatedTemplates": [
      "interface/component/navbar",
      "library/material-ui/navbar"
    ]
  },
  "actions": [
    {
      "name": "create-resource",
      "resources": {
        "components": {
          "tailwind-header-section": {
            "name": "Tailwind - Header section",
            "description": "The header section at the top of a page.",
            "schema": "${import('./configuration/header-section-schema.json')}"
          }
        },
        "slots": {
          "tailwind-header-section": {
            "name": "Tailwind - Header section",
            "component": "tailwind-header-section",
            "content": {
              "en": "${import('./configuration/header-section-content.en.json')}"
            }
          }
        }
      }
    }
  ]
}
